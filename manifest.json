{
  "name": "Search Ebay Sold",
  "description": "Searches Ebay for sold items based on highlighted text with context menu.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus"],
